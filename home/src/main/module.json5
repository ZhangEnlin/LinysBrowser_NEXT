{
  "module": {
    "name": "home",
    "type": "entry",
    "description": "$string:module_desc",
    "mainElement": "HomeAbility",
    "deviceTypes": [
      "default",
      "tablet",
      "2in1"
    ],
    "deliveryWithInstall": true,
    "installationFree": false,
    "pages": "$profile:main_pages",
    "abilities": [
      {
        "name": "HomeAbility",
        "srcEntry": "./ets/homeability/HomeAbility.ets",
        "description": "$string:HomeAbility_desc",
        "icon": "$media:layered_image",
        "label": "$string:HomeAbility_label",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background",
        "exported": true,
        "orientation": "auto_rotation_restricted",
        "continuable": true,
        "skills": [
          {
            "entities": [
              "entity.system.home",
              "entity.system.browsable"
            ],
            "actions": [
              "action.system.home",
              "entity.system.browsable",
              "ohos.want.action.viewData"
            ],
            "uris": [
              {
                "scheme": "https",
                "type": "text/plain",
                "linkFeature": "FileOpen"
              },
              {
                "scheme": "http"
              }
            ],
            "domainVerify": true
          }
        ],
        "minWindowHeight": 100,
        "minWindowWidth": 200
      }
    ],
    "extensionAbilities": [
      {
        "name": "HomeBackupAbility",
        "srcEntry": "./ets/homebackupability/HomeBackupAbility.ets",
        "type": "backup",
        "exported": false,
        "metadata": [
          {
            "name": "ohos.extension.backup",
            "resource": "$profile:backup_config"
          }
        ]
      }
    ],
    "requestPermissions": [
      {
        "name": "ohos.permission.INTERNET",
        "reason": "$string:permission_reason_INTERNET",
        "usedScene": {
          "when": "inuse",
          "abilities": [
            "HomeAbility"
          ]
        }
      },
//            {
//              "name" : "ohos.permission.READ_PASTEBOARD",
//              "reason": "$string:permission_reason_READ_PASTEPOARD",
//              "usedScene": {
//                "when": "inuse",
//                "abilities": [
//                  "HomeAbility"
//                ]
//              }
//            }
    ]
  }
}